{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.wordWrapColumn": 120,
  "editor.stickyScroll.enabled": true,
  "git.autofetch": true,
  "search.useIgnoreFiles": true,
  "search.exclude": {
    "**\/*.code-search": true,
    "**\/node_modules": true,
    "**\/bower_components": true,
    "**/.next": true,
    "**/dist": true,
    "**debug": true,
    "**/package-lock.json": true,
    "**/data": true,
  },
  "[php]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "workbench.iconTheme": "material-icon-theme",
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "window.openFilesInNewWindow": "on",
  "telemetry.telemetryLevel": "off",
  "editor.tabCompletion": "on",
  "extensions.autoUpdate": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "files.insertFinalNewline": false,
    "editor.wordWrap": "on"
  },
  "[csharp]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "explorer.compactFolders": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "go.useLanguageServer": true,
  "ruby.lint": {
    "standard": true
  },
  "ruby.useBundler": false,
  "ruby.format": "standard",
  "ruby.intellisense": "rubyLocate",
  "ruby.codeCompletion": "rcodetools",
  "ruby.useLanguageServer": true,
  "solargraph.definitions": true,
  "editor.insertSpaces": true,
  "editor.wordWrap": "off",
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.startupEditor": "none",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80,
    120,
    160
  ],
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "cSpell.userWords": [
    "amplemarket",
    "concat",
    "hubspot",
    "linkedIn",
    "numericality",
    "perseverant",
    "timespan",
    "titlebar"
  ],
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.toolsManagement.autoUpdate": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.insertSpaces": false,
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.accessibilitySupport": "off",
  "prettier.printWidth": 120,
  "files.associations": {
    "*.gohtml": "html",
    "*.erb": "html"
  },
  "cSpell.enableFiletypes": [
    "haml",
    "ruby"
  ],
  "remote.SSH.remotePlatform": {
    "ack": "linux"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "testdouble.vscode-standard-ruby"
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[plaintext]": {
    "files.insertFinalNewline": false,
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false,
    "ruby": true,
    "javascriptreact": true,
    "javascript": false
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontSize": 14,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.semanticHighlighting.enabled": true,
  "editor.tabSize": 2,
  "settingsSync.ignoredExtensions": [
    "rust-lang.rust-analyzer",
    "ms-dotnettools.csharp",
    "golang.go",
    "xdebug.php-debug"
  ],
  "workbench.colorCustomizations": {
    "[Tokyo Night Storm]": {
      "sideBar.background": "#1c203280",
      "sideBar.foreground": "#959cbd",
      "panel.background": "#1c2032",
      "editor.background": "#202435",
      "tab.activeForeground": "#bdc7f0",
      "breadcrumb.activeSelectionForeground": "#bdc7f0",
      "list.focusForeground": "#bdc7f0",
      "list.hoverForeground": "#bdc7f0",
      "list.activeSelectionForeground": "#bdc7f0",
      "dropdown.foreground": "#959cbd",
      "menu.foreground":"#bdc7f0",
      "menubar.selectionForeground":"#bdc7f0",
      "input.foreground": "#959cbd",
      "activityBar.foreground": "#bdc7f0",
      "activityBar.background": "#1c203280",
      "statusBar.background": "#1c203280"
    },
    "activityBar.background": "#1c203280",
    "sideBar.background": "#1c203280",
    "statusBar.background": "#1c203280"
  },
  "rubyLsp.enabledFeatures": {
    "codeActions": true,
    "diagnostics": false,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": false,
    "hover": true,
    "inlayHint": true,
    "onTypeFormatting": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "completion": true,
    "codeLens": true,
    "definition": true,
    "workspaceSymbol": true,
    "signatureHelp": true,
    "references": true
  },
  "window.commandCenter": false,
  "rubyLsp.formatter": "none",
  "rubyLsp.enableExperimentalFeatures": false,
  "workbench.colorTheme": "Tokyo Night Storm",
  "errorLens.enabled": true,
  "errorLens.excludeBySource": [
    "cSpell"
  ],
  "sqltools.results.reuseTabs": "connection",
  "rewrap.autoWrap.enabled": true,
  "rewrap.wrappingColumn": 120,
  "zig.path": "",
  "zig.zls.path": "/Users/acosta/Library/Application Support/Code/User/globalStorage/ziglang.vscode-zig/zls_install/zls",
  "zig.initialSetupDone": true,
  "editor.lineHeight": 1.6,
  "window.titleBarStyle": "native",
  "window.customTitleBarVisibility": "never",
  "apc.electron": {
		"titleBarStyle": "hiddenInset",
		"backgroundColor": "#00000000",
		"transparent": true,
    "vibrancy": "under-window"
	},
  "apc.font.family": "JetBrainsMono Nerd Font",
  "apc.statusBar": {
		"height": 32
	},
	"apc.header": {
		"height": 42
	},
	"apc.activityBar": {
		"size": 42,
		"itemSize": 36,
    "itemMargin": 0,
		"position": "top"
  },
  "apc.stylesheet": {
    ".activitybar.left .content": "-webkit-app-region: none !important",
    ".activitybar.left .composite-bar .monaco-action-bar": "margin-left: 10px"
  },
  "standardRuby.mode": "enableViaGemfile",
}