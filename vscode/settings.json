{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.wordWrapColumn": 120,
  "editor.stickyScroll.enabled": true,
  "git.autofetch": true,
  "search.exclude": {
    "**\/node_modules": true,
    "**\/*.code-search": true,
    "**\/bower_components": true
  },
  "[php]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.enablePreview": false,
  "workbench.layoutControl.enabled": false,
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.fontFamily": "'JetBrains Mono', 'Fira Code', Menlo, sans-serif",
  "window.openFilesInNewWindow": "on",
  "telemetry.telemetryLevel": "off",
  "editor.tabCompletion": "on",
  "extensions.autoUpdate": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "files.insertFinalNewline": false,
    "editor.wordWrap": "on"
  },
  "[csharp]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "explorer.compactFolders": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "go.useLanguageServer": true,
  "ruby.lint": {
    "standard": true
  },
  "ruby.useBundler": false,
  "ruby.format": "standard",
  "ruby.intellisense": "rubyLocate",
  "ruby.codeCompletion": "rcodetools",
  "ruby.useLanguageServer": true,
  "solargraph.definitions": true,
  "editor.insertSpaces": true,
  "editor.wordWrap": "off",
  "workbench.editor.showTabs": "multiple",
  "workbench.startupEditor": "none",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80,
    120,
    160
  ],
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "cSpell.userWords": [
    "amplemarket",
    "concat",
    "hubspot",
    "linkedIn",
    "numericality",
    "timespan",
    "titlebar"
  ],
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.toolsManagement.autoUpdate": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.insertSpaces": false,
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.accessibilitySupport": "off",
  "prettier.printWidth": 120,
  "window.openFoldersInNewWindow": "on",
  "files.associations": {
    "*.gohtml": "html",
    "*.erb": "html"
  },
  "cSpell.enableFiletypes": [
    "haml",
    "ruby"
  ],
  "remote.SSH.remotePlatform": {
    "ack": "linux"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "testdouble.vscode-standard-ruby"
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[plaintext]": {
    "files.insertFinalNewline": false,
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false,
    "ruby": true,
    "javascriptreact": true,
    "javascript": false
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.fontFamily": "'JetBrainsMono NF', 'JetBrainsMono Nerd Font', 'FiraMono Nerd Font', 'CaskaydiaCove Nerd Font Mono', monospace",
  "terminal.integrated.fontSize": 13,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.menuBarVisibility": "toggle",
  "editor.semanticHighlighting.enabled": true,
  "editor.tabSize": 2,
  "settingsSync.ignoredExtensions": [
    "rust-lang.rust-analyzer",
    "ms-dotnettools.csharp",
    "golang.go",
    "xdebug.php-debug"
  ],
  "workbench.colorCustomizations": {
    "[Tokyo Night Storm]": {
      "sideBar.background": "#1c2032",
      "sideBar.foreground": "#959cbd",
      "panel.background": "#1c2032",
      "editor.background": "#202435",
      "tab.activeForeground": "#bdc7f0",
      "breadcrumb.activeSelectionForeground": "#bdc7f0",
      "list.focusForeground": "#bdc7f0",
      "list.hoverForeground": "#bdc7f0",
      "list.activeSelectionForeground": "#bdc7f0",
      "dropdown.foreground": "#959cbd",
      "menu.foreground":"#bdc7f0",
      "menubar.selectionForeground":"#bdc7f0",
      "input.foreground": "#959cbd",
      "activityBar.foreground": "#bdc7f0",
    },
  },
  "rubyLsp.enabledFeatures": {
    "codeActions": true,
    "diagnostics": false,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": false,
    "hover": true,
    "inlayHint": true,
    "onTypeFormatting": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "completion": true,
    "codeLens": true,
    "definition": true,
    "workspaceSymbol": true,
    "references": true
  },
  "rubyLsp.formatter": "none",
  "rubyTestExplorer.rspecCommand": "dev test",
  "rubyTestExplorer.logpanel": true,
  "rubyTestExplorer.testFramework": "rspec",
  "rubyTestExplorer.formatterPath": "/app/.vscode/custom_formatter.rb",
  "window.commandCenter": false,
  "workbench.colorTheme": "Moonlight II",
  "workbench.activityBar.location": "top",
}
