{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.wordWrapColumn": 120,
  "editor.fontSize": 13,
  "git.autofetch": true,
  "search.exclude": {
    "**\/node_modules": true,
    "**\/*.code-search": true,
    "**\/bower_components": true
  },
  "[php]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "workbench.iconTheme": "material-icon-theme",
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "editor.tabSize": 2,
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "'FiraMono Nerd Font', 'CaskaydiaCove Nerd Font Mono', monospace",
  "window.openFilesInNewWindow": "on",
  "telemetry.telemetryLevel": "off",
  "workbench.colorTheme": "Moonlight II",
  "editor.tabCompletion": "on",
  "extensions.autoUpdate": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.startupEditor": "none",
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
    "editor.quickSuggestions": false,
    "files.insertFinalNewline": false,
    "editor.wordWrap": "on"
  },
  "editor.renderWhitespace": "none",
  "[csharp]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "explorer.compactFolders": false,
  "explorer.openEditors.visible": 0,
  "git.confirmSync": true,
  "git.enableSmartCommit": true,
  "go.useLanguageServer": true,
  "ruby.lint": {
    "standard": true
  },
  "ruby.useBundler": false,
  "ruby.format": "standard",
  "ruby.useLanguageServer": true,
  "solargraph.formatting": false,
  "solargraph.diagnostics": false,
  "editor.insertSpaces": true,
  "editor.wordWrap": "off",
  "editor.autoIndent": "full",
  "workbench.colorCustomizations": {
    "tab.activeModifiedBorder": "#ff00ff"
  },
  "workbench.editor.showTabs": true,
  "files.trimTrailingWhitespace": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80,
    120,
    160
  ],
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "cSpell.userWords": [
    "Amplemarket",
    "concat",
    "Hubspot",
    "LinkedIn",
    "Numericality",
    "Titlebar"
  ],
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.fontFamily": "'Fira Code', Menlo, sans-serif",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.toolsManagement.autoUpdate": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.insertSpaces": false,
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.accessibilitySupport": "off",
  "prettier.printWidth": 120,
  "window.openFoldersInNewWindow": "on",
  "window.menuBarVisibility": "toggle",
  "files.associations": {
    "*.gohtml": "html",
    "*.erb": "html"
  },
  "cSpell.enableFiletypes": [
    "haml",
    "ruby"
  ],
  "workbench.statusBar.visible": true,
  "terminal.integrated.copyOnSelection": true,
  "remote.SSH.remotePlatform": {
    "ack": "linux"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.formatOnSave": false,
    "editor.formatOnSaveMode": "modificationsIfAvailable"
  },
  "diffEditor.ignoreTrimWhitespace": false,
}