{
  "[csharp]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[go]": {
    "editor.insertSpaces": false
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "other": "off",
      "strings": "off"
    },
    "editor.wordWrap": "on",
    "files.insertFinalNewline": false,
    "files.trimTrailingWhitespace": false
  },
  "[php]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[plaintext]": {
    "files.insertFinalNewline": false
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnSave": true
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "cSpell.enabledFileTypes": {
    "markdown": false
  },
  "cSpell.enableFiletypes": [
    "haml",
    "ruby"
  ],
  "cSpell.userWords": [
    "amplemarket",
    "concat",
    "hubspot",
    "linkedIn",
    "numericality",
    "perseverant",
    "timespan",
    "titlebar"
  ],
  "editor.accessibilitySupport": "off",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.lineHeight": 1.6,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80,
    120,
    160
  ],
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 120,
  "errorLens.enabled": true,
  "errorLens.excludeBySource": [
    "cSpell"
  ],
  "explorer.compactFolders": false,
  "extensions.autoUpdate": true,
  "files.associations": {
    "*.erb": "html",
    "*.gohtml": "html"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true,
    "javascript": false,
    "javascriptreact": true,
    "markdown": false,
    "plaintext": false,
    "ruby": true,
    "yaml": true
  },
  "gitlens.launchpad.indicator.enabled": false,
  "gitlens.launchpad.indicator.polling.enabled": false,
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prettier.printWidth": 120,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "remote.SSH.remotePlatform": {
    "ack": "linux"
  },
  "rewrap.autoWrap.enabled": true,
  "rewrap.wrappingColumn": 120,
  "rubyLsp.enabledFeatures": {
    "codeActions": true,
    "codeLens": true,
    "completion": true,
    "definition": true,
    "diagnostics": true,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": true,
    "hover": true,
    "inlayHint": true,
    "onTypeFormatting": true,
    "references": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "signatureHelp": true,
    "typeHierarchy": true,
    "workspaceSymbol": true
  },
  "rubyLsp.formatter": "standard",
  "rubyLsp.linters": [
    "standard"
  ],
  "rubyLsp.rubyVersionManager": {
    "identifier": "mise"
  },
  "search.exclude": {
    "**/.next": true,
    "**\/*.code-search": true,
    "**\/bower_components": true,
    "**/data": true,
    "**/dist": true,
    "**\/node_modules": true,
    "**/package-lock.json": true,
    "**debug": true
  },
  "search.useIgnoreFiles": true,
  "settingsSync.ignoredExtensions": [
    "rust-lang.rust-analyzer",
    "ms-dotnettools.csharp",
    "golang.go",
    "xdebug.php-debug"
  ],
  "solargraph.definitions": true,
  "sqltools.results.reuseTabs": "connection",
  "standardRuby.mode": "enableViaGemfile",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.smoothScrolling": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vscode-run-rspec-file.custom-command": "bin/rspec --no-profile",
  "window.commandCenter": false,
  "window.openFilesInNewWindow": "on",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "[Tokyo Night Storm]": {
      "activityBar.background": "#1c203280",
      "activityBar.foreground": "#bdc7f0",
      "breadcrumb.activeSelectionForeground": "#bdc7f0",
      "dropdown.foreground": "#959cbd",
      "editor.background": "#202435",
      "input.foreground": "#959cbd",
      "list.activeSelectionForeground": "#bdc7f0",
      "list.focusForeground": "#bdc7f0",
      "list.hoverForeground": "#bdc7f0",
      "menu.foreground": "#bdc7f0",
      "menubar.selectionForeground": "#bdc7f0",
      "panel.background": "#1c2032",
      "sideBar.background": "#1c203280",
      "sideBar.foreground": "#959cbd",
      "statusBar.background": "#1c203280",
      "tab.activeForeground": "#bdc7f0"
    }
  },
  "workbench.colorTheme": "Tokyo Night",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "zig.initialSetupDone": true
}
