{
  "workbench.editorAssociations": {
    "*.ipynb" : "jupyter-notebook"
  },
  "editor.wordWrapColumn" : 120,
  "editor.fontSize" : 13,
  "git.autofetch" : true,
  "search.exclude" : {
    "**\/node_modules" : true,
    "**\/*.code-search" : true,
    "**\/bower_components" : true
  },
  "[php]" : {
    "editor.insertSpaces" : true,
    "editor.tabSize" : 4
  },
  "workbench.iconTheme" : "material-icon-theme",
  "prettier.semi" : false,
  "editor.tabSize" : 2,
  "editor.fontLigatures" : true,
  "terminal.integrated.fontFamily" : "'FiraMono Nerd Font', 'CaskaydiaCove Nerd Font Mono', monospace",
  "window.openFilesInNewWindow" : "on",
  "telemetry.telemetryLevel" : "off",
  "workbench.colorTheme" : "One Dark Pro Darker",
  "prettier.singleQuote" : true,
  "workbench.editor.highlightModifiedTabs" : true,
  "editor.tabCompletion" : "on",
  "ruby.useLanguageServer" : false,
  "workbench.editor.enablePreview" : false,
  "extensions.autoUpdate" : true,
  "[json]" : {
    "editor.defaultFormatter" : "vscode.json-language-features"
  },
  "workbench.startupEditor" : "none",
  "[python]" : {
    "editor.insertSpaces" : true,
    "editor.tabSize" : 4
  },
  "[markdown]" : {
    "files.trimTrailingWhitespace" : false,
    "editor.quickSuggestions" : false,
    "files.insertFinalNewline" : false,
    "editor.wordWrap" : "on"
  },
  "editor.renderWhitespace" : "none",
  "[csharp]" : {
    "editor.insertSpaces" : true,
    "editor.tabSize" : 4
  },
  "explorer.compactFolders" : false,
  "explorer.openEditors.visible" : 0,
  "git.confirmSync" : true,
  "git.enableSmartCommit" : true,
  "go.useLanguageServer": true,
  "ruby.lint" : {
    "rubocop" : {
      "useBundler" : false,
      "command" : "rubocop --require rubocop-rails",
      "except" : [
        "Layout\/MultilineMethodCallIndentation",
        "Style\/Documentation",
        "Style\/StringLiterals"
      ]
    }
  },
  "ruby.useBundler" : false,
  "editor.insertSpaces" : true,
  "editor.wordWrap" : "off",
  "editor.autoIndent" : "full",
  "workbench.colorCustomizations" : {
    "tab.activeModifiedBorder" : "#ff00ff"
  },
  "workbench.editor.showTabs" : true,
  "files.trimTrailingWhitespace" : true,
  "editor.minimap.enabled" : false,
  "editor.rulers" : [
    80,
    120,
    160
  ],
  "[html]" : {
    "editor.defaultFormatter" : "vscode.html-language-features"
  },
  "cSpell.userWords" : [
    "Amplemarket",
    "concat",
    "Hubspot",
    "LinkedIn",
    "Numericality",
    "Titlebar"
  ],
  "notebook.cellToolbarLocation" : {
    "default" : "right",
    "jupyter-notebook" : "left"
  },
  "editor.fontFamily" : "'Fira Code', Menlo, sans-serif",
  "javascript.updateImportsOnFileMove.enabled" : "always",
  "[typescript]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "go.toolsManagement.autoUpdate" : true,
  "[javascript]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "[go]" : {
    "editor.insertSpaces" : false
  },
  "[javascriptreact]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "workbench.editor.enablePreviewFromQuickOpen" : false,
  "editor.accessibilitySupport" : "off",
  "prettier.printWidth": 120,
  "window.openFoldersInNewWindow" : "on",
  "window.menuBarVisibility" : "toggle",
  "files.associations" : {
    "*.gohtml" : "html",
    "*.erb" : "html"
  },
  "cSpell.enableFiletypes" : [
    "haml",
    "ruby"
  ],
  "workbench.statusBar.visible" : true,
  "[jsonc]" : {
    "editor.defaultFormatter" : "vscode.json-language-features"
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "solargraph.formatting": true
}