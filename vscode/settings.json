{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.wordWrapColumn": 120,
  "git.autofetch": true,
  "search.exclude": {
    "**\/node_modules": true,
    "**\/*.code-search": true,
    "**\/bower_components": true
  },
  "[php]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "workbench.colorTheme": "Moonlight II",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.enablePreview": false,
  "workbench.layoutControl.enabled": false,
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "editor.fontSize": 13,
  "editor.tabSize": 2,
  "editor.fontLigatures": true,
  "window.openFilesInNewWindow": "on",
  "window.menuBarVisibility": "toggle",
  "telemetry.telemetryLevel": "off",
  "editor.tabCompletion": "on",
  "extensions.autoUpdate": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "files.insertFinalNewline": false,
    "editor.wordWrap": "on"
  },
  "editor.renderWhitespace": "none",
  "[csharp]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "explorer.compactFolders": false,
  "explorer.openEditors.visible": 0,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "go.useLanguageServer": true,
  "ruby.lint": {
    "standard": true
  },
  "ruby.useBundler": false,
  "ruby.format": "standard",
  "ruby.intellisense": "rubyLocate",
  "ruby.codeCompletion": "rcodetools",
  "ruby.useLanguageServer": true,
  "solargraph.definitions": false,
  "editor.insertSpaces": true,
  "editor.wordWrap": "off",
  "editor.autoIndent": "full",
  "workbench.editor.showTabs": true,
  "workbench.startupEditor": "none",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80,
    120,
    160
  ],
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "cSpell.userWords": [
    "amplemarket",
    "concat",
    "hubspot",
    "linkedIn",
    "numericality",
    "titlebar"
  ],
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.fontFamily": "'JetBrains Mono', 'Fira Code', Menlo, sans-serif",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.toolsManagement.autoUpdate": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.insertSpaces": false,
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.accessibilitySupport": "off",
  "prettier.printWidth": 120,
  "window.openFoldersInNewWindow": "on",
  "files.associations": {
    "*.gohtml": "html",
    "*.erb": "html"
  },
  "cSpell.enableFiletypes": [
    "haml",
    "ruby"
  ],
  "remote.SSH.remotePlatform": {
    "ack": "linux"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "rebornix.ruby"
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": false,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "ruby": true,
    "javascriptreact": true,
    "javascript": false
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.fontFamily": "'JetBrainsMono NF', 'JetBrainsMono Nerd Font', 'FiraMono Nerd Font', 'CaskaydiaCove Nerd Font Mono', monospace",
  "terminal.integrated.fontSize": 13,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}